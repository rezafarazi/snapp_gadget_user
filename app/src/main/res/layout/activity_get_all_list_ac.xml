<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".get_all_list_ac">



        <include
            layout="@layout/header_m_ac"
            android:layout_width="match_parent"
            android:layout_height="?actionBarSize"
            android:id="@+id/header_get_all_list_ac"/>










    <android.support.v4.widget.SwipeRefreshLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/swipe_get_all_list_ac"
        android:layout_below="@+id/header_get_all_list_ac">

        <GridView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/grid_get_all_list_ac"/>

    </android.support.v4.widget.SwipeRefreshLayout>









</RelativeLayout>